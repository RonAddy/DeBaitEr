</div>

<center>
  <div id="img">
    <img src="img/main/main_page_header.jpg" />
  </div>

</center>

<div id="newTopic">
  <a href="/bait/new"><button id="newBtn"><i class="fa fa-plus-circle fa-3x" aria-hidden="true"></i></button></a>
  <h1>NEW TOPIC</h1>
</div>

<div id="feed-container">

  <% data.forEach(data => { %>
  <div>
    <div id="top-bar">
      <h1><%= data.category %></h1>
      <h1><%= data.title %></h1>
    </div>
    <div id="bottom-bar">
    <h1><%= data.description %></h1>
    <h1></h1>
    </div>
    </div>
  <br>
  <% }) %>

